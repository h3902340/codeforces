#ifdef DEBUG
#include <fstream>
#endif
#include <iostream>
#include <vector>
using namespace std;

string to_string(vector<int>& a) {
    int n = a.size();
    string res = to_string(a[0]);
    for (int i = 1; i < n; i++) {
        res += ' ' + to_string(a[i]);
    }
    return res;
}

#define ll long long
vector<int> solve(int n) {
    vector<int> res(n);
    res[n - 1] = 1;
    bool neg = false;
    for (int i = n - 2; i >= 0; i--) {
        if (neg) {
            res[i] = res[i + 1] - i - 1;
        } else {
            res[i] = res[i + 1] + i + 1;
        }
        neg = !neg;
    }
    return res;
}

int main() {
    int t;
    int n, x, y;
#ifdef DEBUG
    ifstream in;
    ifstream out;
    in = ifstream("xxxx/testcases/yyyy_in");
    out = ifstream("xxxx/testcases/yyyy_out");
    cin.rdbuf(in.rdbuf());
#endif
    cin >> t;
#if DEBUG
    int cnt = t;
#endif
    for (int i = 1; i <= t; i++) {
        cin >> n;
        auto res = solve(n);
        string res_str = to_string(res);
#ifdef DEBUG
        string ans_str;
        getline(out, ans_str);
        bool pass = true;
        if (res_str.size() != ans_str.size()) {
            pass = false;
        } else {
            for (int i = 0; i < res_str.size(); i++) {
                if (res_str[i] != ans_str[i]) {
                    pass = false;
                    break;
                }
            }
        }
        if (pass) {
            cout << "\033[1;32m" << "CASE " << i << "\033[0m" << endl;
            cout << res_str << endl;
            cout << ans_str << endl;
        } else {
            cout << "\033[1;31m" << "CASE " << i << "\033[0m" << endl;
            cout << res_str << endl;
            cout << "\033[1;31m" << ans_str << "\033[0m" << endl;
            cnt--;
        }
#else
        cout << res_str << endl;
#endif
    }
#ifdef DEBUG
    if (cnt == t) {
        cout << "\033[1;32m" << "ALL PASS [" << cnt << "/" << t << "]"
             << "\033[0m" << endl;
    } else {
        cout << "\033[1;31m" << "WA [" << cnt << "/" << t << "]"
             << "\033[0m" << endl;
    }
    cin.rdbuf(cin.rdbuf());
    in.close();
    out.close();
#endif
    return 0;
}